{
  "files.associations": {
    "iostream": "cpp"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/GPATH": true,
    "**/GRTAGS": true,
    "**/GTAGS": true,
    "**/*user": true,
    "**/Makefile*": true,
    "**/debug": true,
    "**/release": true,
    "**/Debug": true,
    "**/Release": true,
    "**/*-exe": true,
    "**/*.dll": true,
    "**/*stash": true,
    "**/* bat": true,
    "**/*.cmd": true,
    "**/*cache": true,
    "**/* sdf": true,
    "**/*suo": true,
    "**/Win32": true,
    "**/ipch": true,
    "**/x64": true,
    "**/GeneratedFiles": true,
    "**/*db": true,
    "**/*.dSYM": true,
    "**/*.o": true,
    "**/*.out": true
  },
  "search.exclude": {
    "**/*.dSYM": true,
    "**/*.o": true,
    "**/*.out": true
  },

  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
